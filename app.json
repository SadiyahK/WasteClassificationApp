{
  "expo": {
    "name": "classifier-app",
    "description":"A waste classification app that uses Machine Learning to allow users to discover what material an item is.",
    "slug": "classifier-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "src/assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sadiyah.kn.classifier-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.sadiyah.kn.classifierapp"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
